public class Animal {
    String name;
    static int couter = 0;

    public Animal(String name) {
        this.name = name;
    }
    public static void main(String[] args){
        Cat cat = new Cat("Оди");
        couter++;
        Dog dog = new Dog("Джеси");
        couter++;

        dog.dogRun(444);
        cat.catRun(150);
        dog.dogSwin(10);
        cat.catSwin();
        Animal.Counter();
    }

    public static void Counter(){
        System.out.println("Количевство животных равно: " + couter);
    }
}